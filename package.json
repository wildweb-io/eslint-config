{
  "name": "@wildweb/eslint-config",
  "version": "3.0.3-25",
  "packageManager": "pnpm@8.10.0",
  "description": "ESLint shareable flat config used at Wild Web",
  "type": "module",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "homepage": "https://github.com/wildweb-io/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/wildweb-io/eslint-config/issues"
  },
  "repository": "https://github.com/wildweb-io/eslint-config",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "ts": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "8.52.0"
  },
  "dependencies": {
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@next/eslint-plugin-next": "^14.0.1",
    "@stylistic/eslint-plugin": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-antfu": "^1.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-yml": "^1.10.0",
    "globals": "^13.23.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.0.3",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@wildweb/prettier-config": "^3.0.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.52.0",
    "sucrase": "^3.34.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@wildweb/prettier-config"
}
