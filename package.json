{
  "name": "@wildweb/eslint-config",
  "version": "3.0.0",
  "description": "ESLint shareable configs used at Wild Web",
  "type": "module",
  "license": "MIT",
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-antfu": "^1.0.2",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "globals": "^13.23.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@wildweb/prettier-config": "^3.0.3",
    "bumpp": "^9.2.0",
    "eslint": "^8.52.0",
    "sucrase": "^3.34.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "prettier": "@wildweb/prettier-config",
  "repository": "https://github.com/wildweb-io/eslint-config",
  "homepage": "https://github.com/wildweb-io/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/wildweb-io/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ]
}
